#!/bin/sh env tclkit

cd "$root/src/w/bin"

################################################################################
#### wc.js
################################################################################
puts "making wc.js..."
eval exec cat ../dist/wc.js ../dist/wc.es5.js > ../dist/wc.bundle.js

################################################################################
#### wc.css
################################################################################
puts "making wc.css..."
file copy -force ../dist/wc.css ../dist/wc.bundle.css

puts "ENDED."
