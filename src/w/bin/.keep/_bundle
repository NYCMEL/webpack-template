#!/bin/sh env tclkit

################################################################################
#### wc.js
################################################################################
puts "making wc.react.js..."
exec cat \
    ../dist/wc.es5.js\
    > ../../misc/webpack/src/wc.react.js

################################################################################
#### wc.css
################################################################################
puts "making wc.react.css..."
exec cat  ../dist/wc.css > ../../misc/webpack/src/wc.react.css

puts "ENDED."
