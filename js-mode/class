class Yourname {
    ////////////////////////////////////////////////////////
    //// 
    ////////////////////////////////////////////////////////
    constructor(name) {
	wc.group(\"Yourname.constructor:\", name);

	this.name = name;
	alert(this.name);
	
	wc.groupEnd();
    }
}

