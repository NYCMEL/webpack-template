class Products {
    ////////////////////////////////////////////////////////
    //// 
    ////////////////////////////////////////////////////////
    constructor() {
	this.id = \"NOTSET\"
    }

    ////////////////////////////////////////////////////////
    //// 
    ////////////////////////////////////////////////////////
    init(id) {
	this.id = id;

	alert(this.id);
    }
}

////////////////////////////////////////////////////////
//// USAGE
////////////////////////////////////////////////////////
var p = new Products();
p.init(\"my-product\");