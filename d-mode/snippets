# -*- mode: snippet -*-
# name: d-snippets
# key: d
# type: command  
# --
(fset 'd-snippets (lambda (&optional arg) "Keyboard macro." (interactive "p") (kmacro-exec-ring-item (quote ("cd /Melify/private/emacs/common/snippets/")) arg)))
(execute-kbd-macro "cd /Melify/private/emacs/common/snippets/")
